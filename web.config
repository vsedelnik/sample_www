<configuration xmlns="http://schemas.microsoft.com/.NetConfiguration/v2.0">
    <appSettings/>
	<connectionStrings>
		<add name="MyDbConn" 
			connectionString="Server=localhost\SQLEXPRESS; Database=app_test; User Id=sa; password=sa"
			providerName="System.Data.SqlClient" />
	</connectionStrings>
    <system.webServer>
		<defaultDocument>
            <files>
                <add value="index.aspx" />
                <add value="index.html" />
            </files>
        </defaultDocument>
    </system.webServer>
</configuration>
